  <Entries>
    <Entry>
      <ID>1</ID>
      <DefaultText>Mit stoischem Ausdruck neigt der Schmelztiegelritter seinen Kopf, um dich zu begrüßen.</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>2</ID>
      <DefaultText>„Wir werden die Dunkelheit überstehen, genauso wie wir in der Vergangenheit den Weg zum Sieg beleuchtet haben.“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>3</ID>
      <DefaultText>„Pflicht und Geist.“</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>4</ID>
      <DefaultText>„Unsere Feuer geben den Menschen im Dyrwald Wärme und Hoffnung.“</DefaultText>
      <FemaleText />
    </Entry>
  </Entries>
